{
  "manifest_version": 2,

  "name": "AWS login helper",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
      "19": "icon19.png",
      "38": "icon38.png"
    },
    "default_title": "AWS login helper",
    "default_popup": "index.html"
  },
  "permissions": [
    "storage"
  ]
}
